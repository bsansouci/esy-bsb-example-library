{
  "name": "EsyExampleLibrary",
  "version": "0.0.1",
  "description": "Example using bsb-native inside esy to build a library",
  "esy": {
    "build": [
      ["bsb.exe", "-build-library"]
    ],
    "install": [
      "esy-installer"
    ],
    "buildsInSource": true
  },
  "dependencies": {
    "bs-platform": "bsansouci/bsb-native#esy-support",
    "@opam/ocamlfind": "*",
    "@opam/alcotest": "*",
    "@opam/reason": "^3.0.0"
  },
  "devDependencies": {
    "ocaml": "~4.6.1"
  },
  "peerDependencies": {
    "ocaml": "~4.6.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bsansouci/esy-bsb-example-library.git"
  },
  "author": "bsansouci & schmavery"
}
